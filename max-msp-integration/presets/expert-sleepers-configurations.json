  "max_patch_templates": {
    "basic_cv_output": {
      "objects": [
        {"type": "udpreceive", "port": 7400},
        {"type": "osc-route", "pattern": "/cv"},
        {"type": "unpack", "format": "s s s i"},
        {"type": "route", "destinations": ["module", "instance", "parameter", "value"]},
        {"type": "scale", "input_range": [0, 127], "output_range": [0.0, 1.0]},
        {"type": "dac~", "channels": 8}
      ],
      "connections": [
        {"from": "udpreceive", "to": "osc-route"},
        {"from": "osc-route", "to": "unpack"},
        {"from": "unpack", "to": "route"},
        {"from": "route", "outlet": 3, "to": "scale"},
        {"from": "scale", "to": "dac~"}
      ]
    },
    
    "validation_monitor": {
      "objects": [
        {"type": "live.meter~", "channels": 8},
        {"type": "live.scope~", "channels": 1},
        {"type": "live.numbox", "range": [0, 127]},
        {"type": "live.slider", "range": [0, 127]},
        {"type": "sprintf", "format": "/cv/%s/%d/%s %d"},
        {"type": "udpsend", "host": "127.0.0.1", "port": 7401}
      ],
      "ui_layout": {
        "meters": {"x": 10, "y": 10, "width": 200, "height": 100},
        "scope": {"x": 220, "y": 10, "width": 300, "height": 200},
        "controls": {"x": 10, "y": 120, "width": 200, "height": 80}
      }
    },
    
    "recording_system": {
      "objects": [
        {"type": "sfrecord~", "channels": 8},
        {"type": "buffer~", "length": 60000},
        {"type": "record~", "buffer": "recording_buffer"},
        {"type": "play~", "buffer": "recording_buffer"},
        {"type": "timer", "unit": "ms"}
      ],
      "file_format": {
        "type": "wav",
        "bit_depth": 24,
        "sample_rate": 96000,
        "channels": 8
      }
    }
  },
  
